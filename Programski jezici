

main ()
{
    int s, j;
    int A[4][4];
    int s=0;
    for(s=0;s<4;i++)
    for(j=0;j<4;j++)
    {
    printf("A[%d][%d]:", s, j);
    scanf("%d", &A[i][j]);
    }

    for(s=0;s<4;s++)
    for(j=0;j<4;j++)
    {

          if(s<j)
              {
                s=s+A[s][j];
              }
    }
    printf("suma elemenata iznad glavne dijagonale je :%d, s);
    return 0;
}
