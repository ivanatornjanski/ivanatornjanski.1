

main ()
{
    int i, j;
    int A[4][4];
    int s=0;
    for(i=0;i<4;i++)
    for(j=0;j<4;j++)
    {
    printf("A[%d][%d]:", i, j);
    scanf("%d", &A[i][j]);
    }

    for(i=0;i<4;i++)
    for(j=0;j<4;j++)
    {

          if(i<j)
              {
                s=s+A[i][j];
              }
    }
    printf("suma elemenata iznad glavne dijagonale je :%d, s);
    return 0;
}
